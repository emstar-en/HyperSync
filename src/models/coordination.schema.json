{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "max_dimension": {
      "type": "integer"
    },
    "model": {
      "enum": [
        "euclidean",
        "spherical",
        "hyperbolic",
        "mixed"
      ]
    },
    "node_id": {
      "type": "string"
    },
    "operations": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "precision": {
      "type": "number"
    }
  },
  "required": [
    "node_id",
    "model"
  ],
  "title": "Model Coordination Configuration",
  "type": "object"
}