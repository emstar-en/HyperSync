{
  "description": "Compose a network operator agent from multiple models",
  "name": "agent.compose",
  "parameters": {
    "properties": {
      "capabilities": {
        "description": "List of agent capabilities",
        "items": {
          "type": "string"
        },
        "type": "array"
      },
      "ld_id": {
        "description": "Target domain (optional, uses default if not specified)",
        "type": "string"
      },
      "members": {
        "description": "List of member node IDs",
        "items": {
          "type": "string"
        },
        "minItems": 1,
        "type": "array"
      },
      "name": {
        "description": "Agent name",
        "maxLength": 64,
        "minLength": 1,
        "type": "string"
      }
    },
    "required": [
      "name",
      "capabilities",
      "members"
    ],
    "type": "object"
  },
  "returns": {
    "properties": {
      "agent_id": {
        "type": "string"
      },
      "ld_id": {
        "type": "string"
      }
    },
    "type": "object"
  }
}