{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Data channel subscription and routing",
  "properties": {
    "buffer_size": {
      "minimum": 1,
      "type": "integer"
    },
    "channel_name": {
      "type": "string"
    },
    "compression": {
      "default": false,
      "type": "boolean"
    },
    "data_type": {
      "type": "string"
    },
    "update_frequency": {
      "enum": [
        "realtime",
        "1s",
        "5s",
        "10s",
        "30s",
        "1m"
      ],
      "type": "string"
    }
  },
  "required": [
    "channel_name",
    "data_type"
  ],
  "title": "Data Channel",
  "type": "object"
}