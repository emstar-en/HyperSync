{
  "$id": "https://hypersync.ai/schemas/telemetry/dimensional_event_schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Telemetry event for dimensional operations",
  "properties": {
    "dimensional_context": {
      "properties": {
        "curvature": {
          "type": "number"
        },
        "dimensions": {
          "items": {
            "type": "integer"
          },
          "type": "array"
        },
        "frame_id": {
          "type": "string"
        },
        "session_id": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "event_id": {
      "description": "Unique event identifier",
      "type": "string"
    },
    "event_type": {
      "description": "Event type",
      "enum": [
        "dim.sync.start",
        "dim.sync.complete",
        "dim.sync.failed",
        "dim.align.start",
        "dim.align.complete",
        "vector.query",
        "contract.created",
        "route.negotiated",
        "policy.evaluated"
      ],
      "type": "string"
    },
    "metadata": {
      "properties": {
        "span_id": {
          "type": "string"
        },
        "tags": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "trace_id": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "metrics": {
      "properties": {
        "convergence_error": {
          "type": "number"
        },
        "duration_ms": {
          "type": "number"
        },
        "error_rate": {
          "type": "number"
        },
        "latency_ms": {
          "type": "number"
        },
        "throughput_mbps": {
          "type": "number"
        }
      },
      "type": "object"
    },
    "source": {
      "properties": {
        "agent_id": {
          "type": "string"
        },
        "node_id": {
          "type": "string"
        },
        "user_id": {
          "type": "string"
        }
      },
      "required": [
        "node_id"
      ],
      "type": "object"
    },
    "timestamp": {
      "description": "Event timestamp",
      "format": "date-time",
      "type": "string"
    }
  },
  "required": [
    "event_id",
    "event_type",
    "timestamp",
    "source"
  ],
  "title": "Dimensional Event Schema",
  "type": "object"
}