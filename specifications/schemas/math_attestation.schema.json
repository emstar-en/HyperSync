{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "attestation_id": {
      "type": "string"
    },
    "component": {
      "enum": [
        "surreal",
        "hyperbolic",
        "integrated"
      ]
    },
    "timestamp": {
      "type": "string"
    },
    "verdict": {
      "enum": [
        "pass",
        "fail",
        "warning"
      ]
    }
  },
  "required": [
    "attestation_id",
    "timestamp",
    "component",
    "verdict"
  ],
  "title": "Mathematical Attestation",
  "type": "object"
}