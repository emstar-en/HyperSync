{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "attestation": {
      "type": "object"
    },
    "local_ico": {
      "type": "object"
    },
    "policy": {
      "type": "object"
    },
    "remote_ico": {
      "type": "object"
    },
    "session_id": {
      "type": "string"
    },
    "state": {
      "enum": [
        "init",
        "hello_sent",
        "hello_recv",
        "negotiating",
        "active",
        "degraded",
        "closing",
        "closed",
        "error"
      ]
    }
  },
  "required": [
    "session_id",
    "local_ico",
    "remote_ico",
    "policy",
    "state"
  ],
  "title": "ICO Session Configuration",
  "type": "object"
}