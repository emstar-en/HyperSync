{
  "$id": "https://hypersync.ai/schemas/math/valuation_compare.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Primitive for comparing values via valuation-theoretic ordering",
  "properties": {
    "comparison_id": {
      "type": "string"
    },
    "deterministic": {
      "description": "Whether comparison is deterministic",
      "type": "boolean"
    },
    "kind": {
      "const": "ValuationCompare",
      "type": "string"
    },
    "operands": {
      "items": {
        "properties": {
          "operand_id": {
            "type": "string"
          },
          "valuation": {
            "properties": {
              "hahn_series": {
                "type": "string"
              },
              "ordered_group_element": {
                "type": "string"
              }
            },
            "type": "object"
          },
          "value": {
            "type": "string"
          }
        },
        "required": [
          "operand_id",
          "value",
          "valuation"
        ],
        "type": "object"
      },
      "minItems": 2,
      "type": "array"
    },
    "result": {
      "properties": {
        "minimal_element": {
          "type": "string"
        },
        "ordering": {
          "enum": [
            "less_than",
            "equal",
            "greater_than",
            "incomparable"
          ],
          "type": "string"
        },
        "proof": {
          "properties": {
            "method": {
              "type": "string"
            },
            "steps": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          },
          "type": "object"
        }
      },
      "required": [
        "ordering",
        "minimal_element"
      ],
      "type": "object"
    }
  },
  "required": [
    "kind",
    "comparison_id",
    "operands",
    "result"
  ],
  "title": "ValuationCompare",
  "type": "object"
}