{
  "capsule": {
    "name": "hyperbolic_space",
    "type": "manifold",
    "properties": {
      "model": "Poincare Ball Model (D^n)",
      "metric_tensor": "g_ij = (2 / (1 - ||x||^2))^2 delta_ij",
      "distance_function": "dist_M(u, v) = arccosh(1 + 2 * ||u-v||^2 / ((1-||u||^2)(1-||v||^2)))",
      "constraints": "||x|| < 1"
    },
    "vnes_binding": "geometry.manifolds.hyperbolic"
  }
}