{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "determinism_required": {
      "type": "boolean",
      "default": true,
      "description": "Whether execution must be deterministic"
    },
    "tier_gating": {
      "type": "string",
      "enum": [
        "off",
        "soft",
        "hard"
      ],
      "default": "off",
      "description": "Level of tier-based feature gating"
    },
    "privacy_enforced": {
      "type": "boolean",
      "default": true,
      "description": "Whether privacy rules are enforced"
    }
  }
}