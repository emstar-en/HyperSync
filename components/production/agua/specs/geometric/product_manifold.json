{
  "specification_name": "H⁴ × S³ × E⁵ Product Manifold",
  "version": "1.0.0",
  "description": "Complete product manifold structure unifying hyperbolic, spherical, and Euclidean spaces",
  "domain": "Unified AGUA 12D",
  "dimension": 12,
  "curvature": "Mixed (-1, +1, 0)",
  "curvature_type": "product",
  "components": [
    {
      "name": "H⁴",
      "dimension": 4,
      "curvature": -1,
      "domain": "Physical/Spacetime"
    },
    {
      "name": "S³",
      "dimension": 3,
      "curvature": 1,
      "domain": "Abstract/Conceptual"
    },
    {
      "name": "E⁵",
      "dimension": 5,
      "curvature": 0,
      "domain": "Informational/Computational"
    }
  ],
  "types": [
    {
      "name": "ProductPoint",
      "description": "Point in H⁴ × S³ × E⁵",
      "structure": {
        "h4_component": "H4Point (5 coords)",
        "s3_component": "S3Point (4 coords)",
        "e5_component": "E5Point (5 coords)"
      },
      "total_dimension": 14,
      "effective_dimension": 12
    }
  ],
  "operations": [
    {
      "name": "product_point_creation",
      "description": "Create point in product manifold",
      "inputs": [
        {
          "name": "h4",
          "type": "H4Point"
        },
        {
          "name": "s3",
          "type": "S3Point"
        },
        {
          "name": "e5",
          "type": "E5Point"
        }
      ],
      "outputs": [
        {
          "name": "point",
          "type": "ProductPoint"
        }
      ],
      "formula": "(h4, s3, e5) ∈ H⁴ × S³ × E⁵",
      "complexity": "O(1)"
    },
    {
      "name": "product_inner_product",
      "description": "Inner product on product tangent space",
      "inputs": [
        {
          "name": "p",
          "type": "ProductPoint"
        },
        {
          "name": "v",
          "type": "ProductTangentVector"
        },
        {
          "name": "w",
          "type": "ProductTangentVector"
        }
      ],
      "outputs": [
        {
          "name": "result",
          "type": "float"
        }
      ],
      "formula": "⟨(v_h, v_s, v_e), (w_h, w_s, w_e)⟩ = ⟨v_h, w_h⟩_H⁴ + ⟨v_s, w_s⟩_S³ + ⟨v_e, w_e⟩_E⁵",
      "complexity": "O(1)"
    },
    {
      "name": "product_distance",
      "description": "Distance on product manifold",
      "inputs": [
        {
          "name": "p",
          "type": "ProductPoint"
        },
        {
          "name": "q",
          "type": "ProductPoint"
        }
      ],
      "outputs": [
        {
          "name": "distance",
          "type": "float"
        }
      ],
      "formula": "d((p_h,p_s,p_e), (q_h,q_s,q_e)) = √(d_H⁴²(p_h,q_h) + d_S³²(p_s,q_s) + d_E⁵²(p_e,q_e))",
      "complexity": "O(1)"
    }
  ],
