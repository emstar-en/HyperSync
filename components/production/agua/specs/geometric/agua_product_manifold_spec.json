  "operations": [
    {
      "id": "QW-PM.1",
      "name": "product_point_creation",
      "description": "Create point in H\u2074 \u00d7 S\u00b3 \u00d7 E\u2075 product manifold",
      "category": "primitive",
      "parameters": [
        {
          "name": "h4_component",
          "type": "HyperboloidPoint",
          "description": "H\u2074 component"
        },
        {
          "name": "s3_component",
          "type": "S3Point",
          "description": "S\u00b3 component"
        },
        {
          "name": "e5_component",
          "type": "E5Point",
          "description": "E\u2075 component"
        }
      ],
      "returns": {
        "type": "ProductPoint",
        "description": "Point in H\u2074 \u00d7 S\u00b3 \u00d7 E\u2075"
      },
      "formula": {
        "expression": "p = (h, s, e) where h \u2208 H\u2074, s \u2208 S\u00b3, e \u2208 E\u2075",
        "validation": [
          "\u27e8h, h\u27e9_Lorentz = -1 (H\u2074 constraint)",
          "\u27e8s, s\u27e9_Euclidean = 1 (S\u00b3 constraint)",
          "e \u2208 \u211d\u2075 (E\u2075 constraint)"
        ]
      },
      "mathematical_properties": [
        "Total dimension: 12 = 4 + 3 + 5",
        "Product structure: M = H\u2074 \u00d7 S\u00b3 \u00d7 E\u2075",
        "Independence: Components are independent coordinates"
      ],
      "edge_cases": [
        {
          "case": "invalid_h4",
          "description": "H\u2074 point not on hyperboloid",
          "handling": "Error: InvalidH4Point"
        },
        {
          "case": "invalid_s3",
          "description": "S\u00b3 point not on sphere",
          "handling": "Error: InvalidS3Point"
        },
        {
          "case": "invalid_e5",
          "description": "E\u2075 component non-finite",
          "handling": "Error: InvalidE5Point"
        }
      ],
      "complexity": {
        "time": "O(1)",
        "space": "O(1)"
      },
      "synthetic_tests": [
        {
          "name": "standard_product_point",
          "test_cases": [
            {
              "input": {
                "h4_component": {
                  "t": 1,
                  "x": 0,
                  "y": 0,
                  "z": 0
                },
                "s3_component": {
                  "w": 1,
                  "x": 0,
                  "y": 0,
                  "z": 0
                },
                "e5_component": [
                  0,
                  0,
                  0,
                  0,
                  0
                ]
              },
              "expected": "Valid ProductPoint"
            }
          ]
        }
      ]
    },
    {
      "id": "QW-PM.2",
      "name": "product_inner_product",
      "description": "Compute inner product on product manifold tangent space",
      "category": "primitive",
      "parameters": [
        {
          "name": "base_point",
          "type": "ProductPoint",
          "description": "Base point"
        },
        {
          "name": "v1",
          "type": "ProductTangentVector",
          "description": "First tangent vector"
        },
        {
          "name": "v2",
          "type": "ProductTangentVector",
          "description": "Second tangent vector"
        }
      ],
      "returns": {
        "type": "float64",
        "description": "Inner product"
      },
      "formula": {
        "expression": "\u27e8v1, v2\u27e9_product = \u27e8v1_H, v2_H\u27e9_H\u2074 + \u27e8v1_S, v2_S\u27e9_S\u00b3 + \u27e8v1_E, v2_E\u27e9_E\u2075",
        "components": {
          "h4_part": "Lorentz inner product on H\u2074 component",
          "s3_part": "Euclidean inner product on S\u00b3 component",
          "e5_part": "Euclidean inner product on E\u2075 component"
        }
      },
      "mathematical_properties": [
        "Symmetry: \u27e8v1, v2\u27e9 = \u27e8v2, v1\u27e9",
        "Linearity: Bilinear form",
        "Decomposition: Sum of component inner products"
      ],
      "complexity": {
        "time": "O(1)",
        "space": "O(1)"
      }
    },
    {
      "id": "QW-PM.3",
      "name": "product_distance",
      "description": "Compute distance on product manifold",
      "category": "primitive",
      "parameters": [
        {
          "name": "p1",
          "type": "ProductPoint",
          "description": "First point"
        },
        {
          "name": "p2",
          "type": "ProductPoint",
          "description": "Second point"
        }
      ],
      "returns": {
        "type": "float64",
        "description": "Distance"
      },
      "formula": {
        "expression": "d(p1, p2) = \u221a(d_H\u2074(h1,h2)\u00b2 + d_S\u00b3(s1,s2)\u00b2 + d_E\u2075(e1,e2)\u00b2)",
        "components": {
          "h4_dist": "d_H\u2074(h1, h2) using hyperbolic distance",
          "s3_dist": "d_S\u00b3(s1, s2) using spherical distance",
          "e5_dist": "d_E\u2075(e1, e2) using Euclidean distance"
        }
      },
      "mathematical_properties": [
        "Metric: Satisfies metric axioms",
        "Product metric: Induced by component metrics",
        "Triangle inequality: d(p1,p3) \u2264 d(p1,p2) + d(p2,p3)"
      ],
      "complexity": {
        "time": "O(1)",
        "space": "O(1)"
      }
    },
    {
      "id": "QW-PM.4",
      "name": "product_exponential_map",
      "description": "Exponential map on product manifold",
      "category": "manifold",
      "parameters": [
        {
          "name": "base_point",
          "type": "ProductPoint"
        },
        {
          "name": "tangent_vector",
          "type": "ProductTangentVector"
        }
      ],
      "returns": {
        "type": "ProductPoint"
      },
      "formula": {
        "expression": "exp_p(v) = (exp_H\u2074(v_H), exp_S\u00b3(v_S), exp_E\u2075(v_E))",
        "components": {
          "h4_exp": "Hyperbolic exponential map",
          "s3_exp": "Spherical exponential map",
          "e5_exp": "Euclidean exponential map (translation)"
        }
      },
      "mathematical_properties": [
        "Component-wise: Apply exponential map to each component",
        "Geodesics: Product geodesic is (\u03b3_H, \u03b3_S, \u03b3_E)"
      ],
      "complexity": {
        "time": "O(1)",
        "space": "O(1)"
      }
    },
    {
      "id": "QW-PM.5",
