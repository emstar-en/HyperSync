      "formula": {
        "expression": "\u03b3(t) = (\u03b3_H\u2074(t), \u03b3_S\u00b3(t), \u03b3_E\u2075(t))",
        "note": "Product of component geodesics"
      },
      "complexity": {
        "time": "O(n)",
        "space": "O(n)"
      }
    },
