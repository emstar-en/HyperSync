      },
      "complexity": {
        "time": "O(1)",
        "space": "O(1)"
      }
    },
    {
      "id": "QW-PM.7",
      "name": "product_geodesic",
      "description": "Compute geodesic on product manifold",
      "category": "manifold",
      "parameters": [
        {
          "name": "start_point",
          "type": "ProductPoint"
        },
        {
          "name": "end_point",
          "type": "ProductPoint"
        },
        {
          "name": "num_points",
          "type": "uint32"
        }
