{
  "name": "AGUA Ontological Framework",
  "version": "1.0.0",
  "description": "Philosophical foundations and ontological type system for Adaptive Unified Geometric Architecture",
  "philosophical_foundations": {
    "ontological_commitment": {
      "stance": "Geometric realism",
      "description": "Reality has intrinsic geometric structure that can be modeled mathematically",
      "principles": [
        "Geometric unity - all phenomena have geometric representation",
        "Scale invariance - geometric laws hold across scales",
        "Relational ontology - entities defined by geometric relations",
        "Emergent complexity - higher-level structures emerge from geometry"
      ]
    },
    "epistemological_stance": {
      "stance": "Geometric empiricism",
      "description": "Knowledge acquired through geometric relations and measurements",
      "methods": [
        "Metric measurement - distances and angles",
        "Curvature detection - intrinsic vs extrinsic curvature",
        "Topological invariants - fundamental shape properties",
        "Flow dynamics - evolution and transformation"
      ]
    },
    "metaphysical_framework": {
      "stance": "Process geometry",
      "description": "Reality as geometric flow and transformation",
      "core_concepts": [
        "Becoming over being - processes fundamental",
        "Geometric continuity - smooth transitions",
        "Multi-scale reality - nested hierarchies",
        "Relational existence - no isolated entities"
      ]
    }
  },
  "ontological_types": {
    "foundational_types": [
      {
        "name": "GeometricEntity",
        "level": "abstract",
        "description": "Base ontological type for all geometric objects",
        "properties": {
          "existence": "Has geometric representation",
          "identity": "Uniquely identifiable in geometric space",
          "persistence": "Maintains identity through transformations"
        },
        "subtypes": [
          "ManifoldPoint",
          "TangentElement",
          "GeometricStructure"
        ]
      },
      {
        "name": "ManifoldPoint",
        "level": "concrete",
        "parent": "GeometricEntity",
        "description": "Point on a manifold with intrinsic geometric structure",
        "instances": {
          "H4Point": {
            "manifold": "H\u2074",
            "properties": [
              "hyperbolic",
              "spacetime-like"
            ]
          },
          "S3Point": {
            "manifold": "S\u00b3",
            "properties": [
              "spherical",
              "quaternionic"
            ]
          },
          "E5Point": {
            "manifold": "E\u2075",
            "properties": [
              "flat",
              "informational"
            ]
          },
          "ProductPoint": {
            "manifold": "H\u2074\u00d7S\u00b3\u00d7E\u2075",
            "properties": [
              "composite",
              "12-dimensional"
            ]
          }
        }
      },
      {
        "name": "TangentElement",
        "level": "concrete",
        "parent": "GeometricEntity",
        "description": "Element of tangent space representing directions and velocities",
        "properties": {
          "base_attachment": "Attached to specific manifold point",
          "vector_space": "Forms vector space at each point",
          "transformation_rules": "Transforms under pushforward/pullback"
        }
      },
      {
        "name": "GeometricStructure",
        "level": "abstract",
        "parent": "GeometricEntity",
        "description": "Higher-order structure defining geometric properties",
        "subtypes": [
          "Metric",
          "Connection",
          "CurvatureTensor"
        ]
      }
    ],
    "ontological_domains": [
      {
