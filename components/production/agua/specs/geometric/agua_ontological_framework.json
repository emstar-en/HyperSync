  "ontological_types": [
    {
      "name": "GeometricEntity",
      "description": "Base type for all geometric objects",
      "properties": [
        "manifold: Manifold (H\u2074, S\u00b3, E\u2075, or Product)",
        "coordinates: array",
        "tangent_space: TangentSpace"
      ],
      "invariants": [
        "Must satisfy manifold constraints",
        "Coordinates must be valid for manifold type"
      ]
    },
    {
      "name": "ManifoldPoint",
      "description": "Point on a manifold",
      "subtypes": [
        "H4Point: Point on H\u2074 hyperboloid",
        "S3Point: Point on S\u00b3 sphere",
        "E5Point: Point in E\u2075 Euclidean",
        "ProductPoint: Point on H\u2074 \u00d7 S\u00b3 \u00d7 E\u2075"
      ],
      "operations": [
        "distance(p, q): Manifold distance",
        "geodesic(p, q, t): Point on geodesic",
        "exp_map(p, v): Exponential map",
        "log_map(p, q): Logarithmic map"
      ]
    },
    {
      "name": "TangentElement",
      "description": "Tangent vector or direction",
      "subtypes": [
        "H4TangentVector: Tangent to H\u2074",
        "S3TangentVector: Tangent to S\u00b3",
        "E5Vector: Vector in E\u2075",
        "ProductTangentVector: Tangent to product"
      ],
      "operations": [
        "norm(v): Vector norm",
        "parallel_transport(v, p, q): Transport along geodesic",
        "inner_product(v, w): Inner product"
      ]
    },
    {
      "name": "GeometricStructure",
      "description": "Metrics, connections, curvature",
      "subtypes": [
        "Metric: Inner product on tangent spaces",
        "Connection: Parallel transport rules",
        "Curvature: Geometric curvature tensor"
      ]
    }
  ],
